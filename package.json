{
  "name": "ngx-metafrenzy",
  "description": "Angular 8 module for setting head tags such as title, meta, and link.",
  "version": "4.0.0",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular 8",
    "meta",
    "meta tags",
    "angular service",
    "angular guard"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jenbuzz/ngx-metafrenzy"
  },
  "bugs": {
    "url": "https://github.com/jenbuzz/ngx-metafrenzy/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test --watch=false",
    "test:coverage": "ng test --watch=false --code-coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "ng lint",
    "publish": "npm publish dist",
    "build:lib": "ng-packagr -p package.json"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "./src/index.ts"
    }
  },
  "peerDependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.2",
    "@angular/cli": "^8.2.2",
    "@angular/common": "^8.2.3",
    "@angular/compiler": "^8.2.3",
    "@angular/compiler-cli": "^8.2.3",
    "@angular/core": "^8.2.3",
    "@angular/language-service": "^8.2.3",
    "@angular/platform-browser": "^8.2.3",
    "@angular/platform-browser-dynamic": "^8.2.3",
    "@angular/router": "^8.2.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.9.0",
    "codelyzer": "^4.0.1",
    "core-js": "^2.6.4",
    "coveralls": "^3.0.2",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "^4.7.0",
    "phantomjs-prebuilt": "^2.1.16",
    "rxjs": "^6.4.0",
    "ts-node": "~8.0.2",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.0",
    "tslint": "~5.12.1",
    "typescript": "3.5.3",
    "zone.js": "^0.9.1"
  }
}
