{
  "name": "ngx-metafrenzy",
  "description": "Angular 9 module for setting head tags such as title, meta, and link.",
  "version": "4.1.0",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular 9",
    "meta",
    "meta tags",
    "angular service",
    "angular guard"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jenbuzz/ngx-metafrenzy"
  },
  "bugs": {
    "url": "https://github.com/jenbuzz/ngx-metafrenzy/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test --watch=false",
    "test:coverage": "ng test --watch=false --code-coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "ng lint",
    "publish": "npm publish dist",
    "build:lib": "ng-packagr -p package.json"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "./src/index.ts"
    }
  },
  "peerDependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "^9.0.4",
    "@angular/common": "^9.0.4",
    "@angular/compiler": "^9.0.4",
    "@angular/compiler-cli": "^9.0.4",
    "@angular/core": "^9.0.4",
    "@angular/language-service": "^9.0.4",
    "@angular/platform-browser": "^9.0.4",
    "@angular/platform-browser-dynamic": "^9.0.4",
    "@angular/router": "^9.0.4",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.7.7",
    "codelyzer": "^5.2.1",
    "core-js": "^2.6.4",
    "coveralls": "^3.0.9",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "^9.0.2",
    "phantomjs-prebuilt": "^2.1.16",
    "rxjs": "^6.5.4",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "3.7.5",
    "zone.js": "~0.10.2"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  }
}
