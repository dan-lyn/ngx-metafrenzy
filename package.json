{
  "name": "ngx-metafrenzy",
  "description": "Angular 10 module for setting head tags such as title, meta, and link.",
  "version": "6.0.0",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular 10",
    "meta",
    "meta tags",
    "angular service",
    "angular guard"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jenbuzz/ngx-metafrenzy"
  },
  "bugs": {
    "url": "https://github.com/jenbuzz/ngx-metafrenzy/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --watch=false --browsers=ChromeHeadless --code-coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "ng lint",
    "publish": "npm publish dist",
    "build:lib": "ng-packagr -p package.json"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "./src/index.ts"
    }
  },
  "peerDependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular/cli": "^10.1.2",
    "@angular/common": "^10.1.2",
    "@angular/compiler": "^10.1.2",
    "@angular/compiler-cli": "^10.1.2",
    "@angular/core": "^10.1.2",
    "@angular/language-service": "^10.1.2",
    "@angular/platform-browser": "^10.1.2",
    "@angular/platform-browser-dynamic": "^10.1.2",
    "@angular/router": "^10.1.2",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.11.1",
    "codelyzer": "^6.0.0",
    "core-js": "^3.6.5",
    "coveralls": "^3.1.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.2.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.1.0",
    "rxjs": "^6.6.3",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "zone.js": "^0.11.1"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
